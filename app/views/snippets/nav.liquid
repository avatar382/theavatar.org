<div class='container'>
  <div class='col-sm-4 col-sm-offset-4'>
    <div class='row text-center'>
      <h1>
        Manny Rodriguez
      </h1>
      <hr>
      <a href='http://github.com/avatar382' target='_blank'>
        {{ 'github.png' | theme_image_tag }}
      </a>
      <a href='https://www.instagram.com/avatar382/' target='_blank'>
        {{ 'instagram.png' | theme_image_tag }}
      </a>
      <a href='mailto:manny@theavatar.org'>
        {{ 'email.jpg' | theme_image_tag }}
      </a>
    </div>
  </div>
  <div class='col-sm-10 col-sm-offset-1'>
    <div class='row'>
      <ul class='nav nav-tabs'>
        <li class="{% if page.slug == 'index' %}active{% endif %}">
          <a href='{% path_to home %}'>
            Here
          </a>
        </li>
        {% for page_in_nav in site.pages %}
        {% if page_in_nav.depth == 1 %}
        <li class='{% if page.fullpath == page_in_nav.fullpath %}active{% endif %}'>
          {% link_to page_in_nav %}
        </li>
        {% endif %}
        {% endfor %}
      </ul>
    </div>
  </div>
</div>
